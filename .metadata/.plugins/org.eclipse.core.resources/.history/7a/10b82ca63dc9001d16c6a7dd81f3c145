package utils;

/*
 * 형재 페이지와 관련된 정보 저장
 * -현재 페이지 번호
 * -페이지당  게시글의 수
 */
public class Criteria {
	private int pageNum; // 현재 페이지 번호
	private int rowsPerPage; // 페이지당 게시글의 수

	// 생성자
	public Criteria() {
		
	}
	
	public Criteria(int pageNum, int rowsPerPage) {
		this.pageNum = pageNum;
		this.rowsPerPage = rowsPerPage;
	}



}
